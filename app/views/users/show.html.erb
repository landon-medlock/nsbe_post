<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @user.name %>
</p>

<p>
  <strong>User name:</strong>
  <%= @user.user_name %>
</p>

<p>
  <strong>Bio:</strong>
  <%= @user.bio %>
</p>

<div>
	<strong>Posts:</strong>
	<% @user.posts.each do |post| %>
	<p><%= post.content %></p>
	<% end %>
</div>


<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
